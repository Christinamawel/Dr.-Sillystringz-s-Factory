@using Factory.Models;
@{
    List<Engineer> engineers = ViewBag.Engineers != null ? ViewBag.Engineers : null;
    List<Machine> machines = ViewBag.Machines != null ? ViewBag.Machines : null;
}

<h1>Dr. Sillystringz Factory tracker</h1>
<h4>Engineers:</h4>
<ul>
@foreach(Engineer engineer in engineers)
{
  <li>@Html.ActionLink($"{engineer.Name}", "Details","Engineer", new { id = engineer.EngineerId })</li>
}
</ul>
<h4>Machines:</h4>
<ul>
@foreach(Machine machine in machines)
{
  <li>@Html.ActionLink($"{machine.Name}", "Details","Machine", new { id = machine.MachineId })</li>
}
</ul>
<p>@Html.ActionLink("Engineers", "Index", "Engineer")</p>
<p>@Html.ActionLink("Machines", "Index", "Machine")</p>